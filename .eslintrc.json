{
    "root": true,
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "extends": ["eslint:recommended", "prettier"],
    "env": {
        "es2021": true,
        "node": true
    },
    "rules": {
        "no-console": "error",
        "camelcase" : "error",
        "no-undef" :"off"
    }
}
